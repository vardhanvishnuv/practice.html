    <script>
let a=26;
let b=55;
 
 document.write(a+b);
 document.write(a-b);
 document.write(a*b);
 document.write(a/b);
 document.write(a%b);
 document.write(a++);
 document.write(++a);
 document.write(--b);
 document.write(b--);

    </script>
   
