    <body>
        <script>
  const marks=90;
let Branch;
switch(true){
    case marks>80:
    branch="BCA";
    break;
    case marks>60:
    branch ="BBA";
    break;
    case marks>70:
    branch ="B.COM";
    break;
    case marks>50:
    branch ="B.SC (STAT)";
    break;
    default:
    Branch="B.SC(MATHS)";
}
document.write("branch");
        </script>
    
    </body>
